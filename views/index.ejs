<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=q, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Welcome to EJS</h1>
    <h2><%= 1+3 + (" w ") + myName %></h2>

    <% if (myName=="Kun") { %>
        <h1>Hey Kun!</h1>
    <% }else{ %>
        <h1>Welcome, No Kun</h1>
    <% } %>

    <% allstudents.map((eachItem)=>{ %>
        <h1><%= eachItem.firstname %></h1>
        <h1><%= eachItem.lastname %></h1>
    <% }) %>
     
</body>
</html>